<!--Tells version of html-->
<!DOCTYPE html>
<html>
    <!--Head Section content-->
    <head>
        <!--Page Title-->
        <title>Page Title</title>
    </head>
    <!--Body Section Content-->
    <body>
        <!--Used to display heading content (h1-h6)-->
        <h1>This is a heading</h1>
        
        <!--Used to display paragraph content-->
        <p>This is a paragraph.</p>
    
    </body>
</html>     
